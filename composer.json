{
  "name": "vertilia/mime-type",
  "description": "Simple library to abstract mime types",
  "license": "BSD-2-Clause",
  "type": "library",
  "autoload": {
    "exclude-from-classmap": [
      "/tests/"
    ],
    "psr-4": {
      "Vertilia\\MimeType\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.0"
    }
  },
  "require": {
    "php": "^7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
