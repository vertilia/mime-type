{
  "name": "vertilia/mime-type",
  "description": "Simple library to abstract mime types",
  "keywords": ["mime", "type", "request", "json", "xml", "text"],
  "license": "BSD-2-Clause",
  "type": "library",
  "autoload": {
    "exclude-from-classmap": [
      "/tests/"
    ],
    "psr-4": {
      "Vertilia\\MimeType\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  }
}
